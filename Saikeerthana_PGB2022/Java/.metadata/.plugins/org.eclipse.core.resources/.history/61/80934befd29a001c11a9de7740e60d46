package saikeerthana;

import java.util.HashMap;
import java.util.HashSet;
import java.util.Map;
import java.util.Set;

public class Test {

	public static void main(String[] args)
	{
		
			Employee e1 = new Employee("Siddhu", 123,"India");
			Employee e2=new Employee("Siddhu", 123,"India");
			System.out.println(e1.equals(e2));
			Set<Employee> hashSet = new HashSet<>();
			hashSet.add(e1);
			hashSet.add(e2);		
			System.out.println("HashSet Elements: " + hashSet);
			Map<String, String> hashMap = new HashMap<>();
			hashMap.put(e1.getName(), "First Employee");
			hashMap.put(e2.getName(), "Second Employee");
			System.out.println("HashMap Elements: " + hashMap);
		
	}	
}
